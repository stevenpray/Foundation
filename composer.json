{
    "name": "pomm-project/foundation",
    "type": "library",
    "description":  "Pomm connection manager for Postgresql",
    "keywords": ["framework", "postgresql", "database", "pomm"],
    "homepage": "http://www.pomm-project.org",
    "license":  "MIT",
    "authors":  [ { "name": "GrÃ©goire HUBERT", "email": "hubert.greg@gmail.com", "homepage": "http://www.coolkeums.org" } ],
    "require": {
        "php": ">=5.5.9",
        "ext-pgsql": "*",
        "league/event": "^2.1",
        "psr/log": "~1.0"
    },
    "require-dev": {
